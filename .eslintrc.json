{
    "extends": [
        "eslint:recommended",
        "standard"
    ],
    "plugins": [
        "vue"
    ],
    "rules": {
        "comma-dangle": "off",
        "indent": ["error", 4],
        "no-console": 1,
        "quote-props": ["error", "consistent-as-needed"],
        "vue/html-indent": ["error", 4]
    }
}
