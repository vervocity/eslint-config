{
    "name": "@vervocity/eslint-config",
    "version": "3.2.1",
    "description": "Vervocity ESLint & Prettier config",
    "type": "module",
    "main": "index.js",
    "scripts": {
        "lint": "npx eslint index.js",
        "lint:fix": "npm run lint -- --fix",
        "prettier": "npx prettier index.js --check",
        "prettier:fix": "npm run prettier -- --write",
        "format": "npm run prettier:fix && npm run lint:fix"
    },
    "repository": {
        "type": "git",
        "url": "git://github.com/vervocity/eslint-config.git"
    },
    "author": "Brandon Bloodgood",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/vervocity/eslint-config/issues"
    },
    "homepage": "https://github.com/vervocity/eslint-config#readme",
    "dependencies": {
        "@eslint/js": "^9.11.1",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-formatter-checkstyle": "^8.40.0",
        "eslint-plugin-react": "^7.37.1",
        "eslint-plugin-vue": "^9.28.0",
        "globals": "^15.9.0",
        "neostandard": "^0.11.6",
        "prettier": "3.3.3",
        "prettier-plugin-organize-imports": "^4.1.0",
        "prettier-plugin-tailwindcss": "^0.6.8",
        "typescript-eslint": "^8.7.0",
        "vue-tsc": "^2.1.6"
    }
}
